<div class="left">
 <h4 class="issue-name <%= @statusClass %>" data-id="<%= @issue.get('id') %>">
   <i class="toggle icon-check<%= '-empty' if @issue.open() %>"></i>
   <a href="<%= @issue.url() %>" data-id="<%= @issue.get('id') %>"><%= @issue.get 'name' %></a>
   </h4>
</div>
<div class="right">
  <p class="date <%= @statusClass %>"><%= @issue.get('due') %></p>
</div>
