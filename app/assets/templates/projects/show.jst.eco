<div id="project">
  <h1><%= @project.get 'name' %></h1>
  <p><%= @project.get 'description' %></p>

  <h2>Issues</h3>
  <div id="issues">
    <% for issue in @project.issues.models: %>
      <div class="issue">
        <h4 class="issue-name"><%= issue.get 'name' %></h5>
        <p><%= issue.get 'description' %></p>
      </div>
    <% end %>
  </div>
</div>
